#   FIBONACCI SERIES


n_terms=int(input("Enter the number of terms:"))

def fibo(n):
   if n <= 1:
       return n
   else:
       return(fibo(n-1) + fibo(n-2))

if n_terms <= 0:       
   print("Plese enter a positive integer")
   
else:
   print("Fibonacci sequence:")
   for i in range(n_terms):
       print(fibo(i))
