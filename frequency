# FREQUENCY OF A STRING-- in REVERSE

def most_frequent(str1):             #Function definition
    revstr1=str1[::-1]               #reverse of string 
    freq={}
    for i in revstr1:
        if i in freq:
            freq[i]+=1
        else:
            freq[i]=1
    return freq

str1='mississippi'
print(most_frequent(str1))
